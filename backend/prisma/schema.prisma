generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  googleId String @unique
  email String @unique
  firstName String
  picture String
  companies Comapnies[]
}

model Comapnies{
  id Int @id @default(autoincrement())
  name String
  address String
  email String @unique
  description String?
  cnpj String @unique
  category String
  website String?
  user User @relation(fields: [userid], references: [id], onDelete: Cascade)
  userid Int
}